alias DTWAIN_LOGGER_PROCW =
extern(Windows) ptrint function(LPCWSTR text, LONG64 userData);

// Error callbacks
alias DTWAIN_ERROR_PROC =
extern(Windows) ptrint function(LONG errorCode, LONG userData);

alias DTWAIN_ERROR_PROC64 =
extern(Windows) ptrint function(LONG errorCode, LONG64 userData);

