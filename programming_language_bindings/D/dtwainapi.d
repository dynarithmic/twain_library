    * TWAIN Identity structure
    *************************************************/
    public struct TW_IDENTITY
    {
        alias TW_UINT16 = ushort;
        alias TW_UINT32 = uint;
        alias TW_STR32  = char[34]; // 32 chars + NULL + padding

        TW_UINT32 Id;

        TW_UINT16 VersionMajor;
        TW_UINT16 VersionMinor;
        TW_UINT16 Language;
        TW_UINT16 Country;

        TW_STR32  Info;
        TW_STR32  ProductName;

        TW_UINT32 ProtocolMajor;
        TW_UINT32 ProtocolMinor;
        TW_UINT32 SupportedGroups;
        TW_UINT32 Manufacturer;
        TW_UINT32 ProductFamily;
    }

    private HMODULE hModule;

