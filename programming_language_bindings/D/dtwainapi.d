        ptrint function(DTWAIN_CALLBACK_PROC64, LONG64)
			DTWAIN_SetCallback64;

    extern(Windows)
        DTWAIN_CALLBACK_PROC function()
			DTWAIN_GetCallback;

    extern(Windows)
        DTWAIN_CALLBACK_PROC64 function()
			DTWAIN_GetCallback64;

    /************ DIB update ************/
