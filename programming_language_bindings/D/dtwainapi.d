			DTWAIN_GetLoggerCallback;

    extern(Windows)
        DTWAIN_LOGGER_PROCA function()
			DTWAIN_GetLoggerCallbackA;

    extern(Windows)
        DTWAIN_LOGGER_PROCW function()
			DTWAIN_GetLoggerCallbackW;

    extern(Windows)
        DTWAIN_BOOL function(DTWAIN_LOGGER_PROC, LONG64)
			DTWAIN_SetLoggerCallback;

    extern(Windows)
        DTWAIN_BOOL function(DTWAIN_LOGGER_PROCA, LONG64)
			DTWAIN_SetLoggerCallbackA;

    extern(Windows)
        DTWAIN_BOOL function(DTWAIN_LOGGER_PROCW, LONG64)
			DTWAIN_SetLoggerCallbackW;

    /************ Error ************/
    extern(Windows)
        DTWAIN_BOOL function(DTWAIN_ERROR_PROC, LONG)
