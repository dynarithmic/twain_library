    /************ GC-safe callback storage ************/
    private DTWAIN_CALLBACK_PROC    _cb32;
    private DTWAIN_CALLBACK_PROC64  _cb64;
    private DTWAIN_DIBUPDATE_PROC   _dibProc;
    private DTWAIN_LOGGER_PROC      _logger;
    private DTWAIN_LOGGER_PROCA     _loggerA;
    private DTWAIN_LOGGER_PROCW     _loggerW;
    private DTWAIN_ERROR_PROC       _errProc;
    private DTWAIN_ERROR_PROC64     _errProc64;

    private void bindFunction(void** slot, string name)
    {
        auto p = GetProcAddress(hModule, toStringz(name));
        enforce(p !is null, "Missing symbol: " ~ name);
        *slot = cast(void*) p;
    }

    /************ Generic binder ************/
    private void bind(T)(ref T fn, string name)
    {
        fn = cast(T) GetProcAddress(hModule, toStringz(name));
        enforce(fn !is null, "Missing symbol: " ~ name);
    }

    /************ Safe wrappers ************/
    ptrint setCallback(DTWAIN_CALLBACK_PROC cb, int userData)
    {
        _cb32 = cb;
        return DTWAIN_SetCallback(cb, userData);
    }

    ptrint setCallback64(DTWAIN_CALLBACK_PROC64 cb, LONG64 userData)
    {
        _cb64 = cb;
        return DTWAIN_SetCallback64(cb, userData);
    }

    void setDibUpdateProc(DTWAIN_DIBUPDATE_PROC cb)
    {
        _dibProc = cb;
        DTWAIN_SetUpdateDibProc(cb);
    }

    void setLogger(DTWAIN_LOGGER_PROC cb, LONG64 userData)
    {
        _logger = cb;
        DTWAIN_SetLoggerCallback(cb, userData);
    }

    void setLoggerA(DTWAIN_LOGGER_PROCA cb, LONG64 userData)
    {
        _loggerA = cb;
        DTWAIN_SetLoggerCallbackA(cb, userData);
    }

    void setLoggerW(DTWAIN_LOGGER_PROCW cb, LONG64 userData)
    {
        _loggerW = cb;
        DTWAIN_SetLoggerCallbackW(cb, userData);
    }

    void setErrorCallback(DTWAIN_ERROR_PROC cb, LONG userData)
    {
        _errProc = cb;
        DTWAIN_SetErrorCallback(cb, userData);
    }

    void setErrorCallback64(DTWAIN_ERROR_PROC64 cb, LONG64 userData)
    {
        _errProc64 = cb;
        DTWAIN_SetErrorCallback64(cb, userData);
    }

